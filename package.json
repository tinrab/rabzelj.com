	"type": "module",
	"scripts": {
		"build": "pnpm run clean && NODE_ENV=production vinxi build",
		"build:vercel": "pnpm run clean && NODE_ENV=production vinxi build --preset vercel && shx cp -r ./data ./.vercel/output/functions/__nitro.func/data",
